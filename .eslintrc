{
  "extends": [
    "airbnb"
  ],
  "plugins": [
    "react",
    "jsx-a11y",
    "import"
  ],
  "rules": {
    "func-names": ["error", "never"],
    "comma-dangle": "off",
    "camelcase": "off",
    "arrow-body-style": [
      "error",
      "as-needed"
    ],
    "global-require": "off",
    "react/prop-types": [
      1,
      {
        "ignore": [
          "navigator",
          "navigation",
          "style",
          "styles",
          "route"
        ]
      }
    ],
    "react/forbid-prop-types": [0],
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "class-methods-use-this": [1],
    "react/jsx-boolean-value": 1,
    "react/jsx-indent-props": [0],
    "react/jsx-pascal-case": 0,
    "indent": [0],
    "no-console": 0
  }
}
